html, body
    box-sizing border-box
    font-size 16px
    background-color white
    height: 4613px
    box-sizing: border-box
    font-family: 'trench', sans-serif
    +below(1024px)
        height: 2560px
    +below(750px)
        height: 1875px
    +below(480px)
        height: 1200px
    +below(320px)
        height: 800px
*, *:before, *:after
    box-sizing inherit

*:focus
    outline 0
